    <Assign sap:VirtualizedContainerService.HintSize="334,64" sap2010:WorkflowViewState.IdRef="Assign_8">
      <Assign.To>
        <OutArgument x:TypeArguments="scg:IEnumerable(sd:DataRow)">[joinOutDTIENUM]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="scg:IEnumerable(sd:DataRow)" xml:space="preserve">[(From t1 In  joinDT2.AsEnumerable()
	Group Join t2 In  joinDT1.AsEnumerable()
	On Convert.ToString(t1.Field(Of Object)("Id")).Trim() Equals Convert.ToString(t2.Field(Of Object)("Id")).Trim()
	Into temp = Group
	From b In temp.DefaultIfEmpty()
 	Let Id = t1.Field(Of Object)("Id")
	Let Name=b.Field(Of Object)("Name")
 	Let Marks = If(t1 Is Nothing, 0, t1.Field(Of Object)("Marks"))
Select  joinOutDT.LoadDataRow(New Object(){ Id, Name,Marks},False)).ToList]</InArgument>
      </Assign.Value>
    </Assign>
